{
  "manifest_version": 3,
  "name": "link-click-checker",
  "version": "1.0",
  "description": "Vue3でChrome拡張を作成したサンプルです。",
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["clickHandler.js"]
    }
  ]
}